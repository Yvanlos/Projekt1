<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="400.0" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane maxHeight="10.0" minHeight="10.0" prefHeight="10.0" prefWidth="200.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Soll das folgende Team wirklich gelöscht werden?">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Pane prefHeight="10.0" prefWidth="200.0" />
      <Text fx:id="teamName" strokeType="OUTSIDE" strokeWidth="0.0" text="Testteam" textAlignment="CENTER" wrappingWidth="400.0">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Pane prefHeight="20.0" prefWidth="200.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Achtung: es werden alle Entwickler, die diesem Team zugeordnet sind gelöscht!" textAlignment="CENTER" wrappingWidth="350.0">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Text>
      <Pane prefHeight="20.0" prefWidth="200.0" />
      <HBox alignment="TOP_CENTER" prefHeight="20.0" prefWidth="400.0">
         <children>
            <Button fx:id="okButton" mnemonicParsing="false" onMouseClicked="#onOkButtonClicked" prefWidth="100.0" text="Ja" />
            <Pane maxHeight="20.0" minHeight="20.0" prefHeight="20.0" prefWidth="100.0" />
            <Button fx:id="cancelButton" defaultButton="true" maxHeight="20.0" mnemonicParsing="false" onMouseClicked="#onCancelButtonClicked" prefWidth="100.0" text="Nein" />
         </children>
      </HBox>
   </children>
</fx:root>
